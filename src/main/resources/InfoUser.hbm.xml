<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
                                   "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.fyxridd.lib.core.api.model">

<class name="InfoUser" table="InfoUser">
    <composite-id>
        <key-property name="name"/>
        <key-property name="flag"/>
    </composite-id>
    <property name="data" length="1024"/>
</class>

</hibernate-mapping>
